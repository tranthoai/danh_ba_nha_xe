{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/api-pack": "1.2.0",
    "symfony/dotenv": "4.3.4",
    "symfony/asset": "4.3.4",
    "symfony/console": "4.3.4",
    "symfony/expression-language": "4.3.4",
    "symfony/form": "4.3.4",
    "symfony/framework-bundle": "4.3.4",
    "symfony/process": "4.3.4",
    "symfony/security-bundle": "4.3.4",
    "symfony/templating": "4.3.4",
    "symfony/twig-bundle": "4.3.4",
    "symfony/validator": "4.3.4",
    "symfony/web-link": "4.3.4",
    "symfony/yaml": "4.3.4",
    "sensio/framework-extra-bundle": "5.4.1",
    "symfony/swiftmailer-bundle": "3.2.8",
    "symfony/flex": "1.4.6",
    "symfony/monolog-bundle": "3.4.0",
    "symfony/orm-pack": "1.0.6",
    "symfony/serializer-pack": "1.0.2",
    "symfony/webpack-encore-bundle": "1.6.2",
    "bluem/tree": "3.1",
    "easycorp/easyadmin-bundle": "2.3.0",
    "facebook/webdriver": "1.7.1",
    "friendsofsymfony/ckeditor-bundle": "2.1.0",
    "friendsofsymfony/rest-bundle": "2.5.0",
    "jms/serializer-bundle": "3.4.1",
    "lexik/jwt-authentication-bundle": "2.6.4",
    "mashape/unirest-php": "3.0.4",
    "nelmio/api-doc-bundle": "3.4.0",
    "nelmio/cors-bundle": "1.5.6",
    "nesbot/carbon": "2.25.0",
    "phpoffice/phpspreadsheet": "1.9.0",
    "ramsey/uuid": "3.8.0",
    "vich/uploader-bundle": "1.10.1",
    "yectep/phpspreadsheet-bundle": "0.0.6",
    "helios-ag/fm-elfinder-bundle": "9.3.1",
    "twig/extensions": "1.5.4",
    "beberlei/doctrineextensions": "1.2.3",
    "stof/doctrine-extensions-bundle": "1.3.0",
    "doctrine/doctrine-fixtures-bundle": "3.2.2",
    "fzaninotto/faker": "1.8.0"
  },
  "require-dev": {
    "symfony/maker-bundle": "1.13.0",
    "symfony/web-server-bundle": "4.3.4",
    "nelmio/alice": "3.5.7",
    "codeception/codeception": "3.1.1",
    "symfony/debug-pack": "1.0.7",
    "symfony/profiler-pack": "1.0.4",
    "symfony/test-pack": "1.0.6",
    "squizlabs/php_codesniffer": "3.5.0",
    "phpmd/phpmd": "2.7.0",
    "friendsofphp/php-cs-fixer": "2.15.3",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.3.*"
    }
  }
}
